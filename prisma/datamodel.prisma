type Product {
  id: ID! @unique
  uuid: String!
  brand: String!
  category: String!
  shoe: String!
  name: String!
  title: String!
  urlKey: String!
  imgURL: String
  releaseDate: DateTime
  retailPrice: Int
  rawData: Json!
  createdAt: DateTime!
  updatedAt: DateTime!
}

type URL {
  id: ID! @unique
  url: String!
  ProductAmount: Int!
  lastPage: Int!
  createdAt: DateTime!
  updateAt: DateTime!
}